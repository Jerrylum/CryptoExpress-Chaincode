{
  "name": "cryptoexpress-chaincode",
  "version": "0.0.1",
  "description": "Crypto Express contract implementation",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "scripts": {
    "format": "prettier . --write --cache",
    "check-format": "prettier . -c --cache",
    "test": "nyc mocha -r ts-node/register src/**/*.spec.ts",
    "start": "fabric-chaincode-node start",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jerrylum/CryptoExpress-Chaincode.git"
  },
  "author": "Jerry Lum",
  "license": "GPL-3.0",
  "dependencies": {
    "fabric-contract-api": "^2.5.4",
    "fabric-shim": "^2.5.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.24",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.12.0"
  },
  "bugs": {
    "url": "https://github.com/Jerrylum/CryptoExpress-Chaincode/issues"
  },
  "homepage": "https://github.com/Jerrylum/CryptoExpress-Chaincode#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage/**",
      "dist/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
