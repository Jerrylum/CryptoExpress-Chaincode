{
  "components": {
    "schemas": {
      "Address": {
        "$id": "Address",
        "type": "object",
        "properties": {
          "hashId": {
            "type": "string"
          },
          "line1": {
            "type": "string"
          },
          "line2": {
            "type": "string"
          },
          "recipient": {
            "type": "string"
          },
          "publicKey": {
            "type": "object"
          }
        },
        "required": ["hashId", "line1", "line2", "recipient", "publicKey"]
      },
      "Courier": {
        "$id": "Courier",
        "type": "object",
        "properties": {
          "hashId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "company": {
            "type": "string"
          },
          "telephone": {
            "type": "string"
          },
          "publicKey": {
            "type": "object"
          }
        },
        "required": ["hashId", "name", "company", "telephone", "publicKey"]
      },
      "Good": {
        "$id": "Good",
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "barcode": {
            "type": "string"
          }
        },
        "required": ["uuid", "name", "barcode"]
      },
      "Stop": {
        "$id": "Stop",
        "type": "object",
        "properties": {
          "address": {
            "type": "string"
          },
          "arrivalDeltaTimestamp": {
            "type": "number"
          },
          "input": {
            "type": "object",
            "additionalProperties": {
              "type": "number"
            }
          },
          "output": {
            "type": "object",
            "additionalProperties": {
              "type": "number"
            }
          },
          "next": {
            "$ref": "Transport"
          }
        },
        "required": ["address", "arrivalDeltaTimestamp", "input", "output"]
      },
      "Transport": {
        "$id": "Transport",
        "type": "object",
        "properties": {
          "courier": {
            "type": "string"
          },
          "info": {
            "type": "string"
          },
          "destination": {
            "$ref": "Stop"
          }
        },
        "required": ["courier", "info", "destination"]
      },
      "ReceiptDetail": {
        "$id": "ReceiptDetail",
        "type": "object",
        "properties": {
          "delta": {
            "type": "object",
            "additionalProperties": {
              "type": "number"
            }
          },
          "timestamp": {
            "type": "number"
          }
        },
        "required": ["delta", "timestamp"]
      },
      "Receipt": {
        "$id": "Receipt",
        "type": "object",
        "properties": {
          "detail": {
            "$ref": "ReceiptDetail"
          },
          "signature": {
            "type": "object"
          }
        },
        "required": ["detail", "signature"]
      },
      "RouteSessionSegment": {
        "$id": "RouteSessionSegment",
        "type": "object",
        "properties": {
          "srcOutgoing": {
            "$ref": "Receipt"
          },
          "courierReceiving": {
            "$ref": "Receipt"
          },
          "courierDelivering": {
            "$ref": "Receipt"
          },
          "dstIncoming": {
            "$ref": "Receipt"
          }
        },
        "required": ["srcOutgoing", "courierReceiving", "courierDelivering", "dstIncoming"]
      },
      "Route": {
        "$id": "Route",
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "goods": {
            "type": "object",
            "additionalProperties": {
              "type": "number"
            }
          },
          "addresses": {
            "type": "object",
            "additionalProperties": {
              "$ref": "Address"
            }
          },
          "couriers": {
            "type": "object",
            "additionalProperties": {
              "$ref": "Courier"
            }
          },
          "source": {
            "$ref": "Stop"
          }
        },
        "required": ["uuid", "goods", "addresses", "couriers", "source"]
      },
      "RouteProposal": {
        "$id": "RouteProposal",
        "type": "object",
        "properties": {
          "route": {
            "$ref": "Route"
          },
          "signatures": {
            "type": "object",
            "additionalProperties": {
              "type": "object"
            }
          }
        },
        "required": ["route", "signatures"]
      },
      "RouteSession": {
        "$id": "RouteSession",
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "segments": {
            "type": "array",
            "items": {
              "$ref": "RouteSessionSegment"
            }
          }
        },
        "required": ["uuid", "segments"]
      }
    }
  }
}
